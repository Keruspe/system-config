[Unit]
Description=GNOME Shell
After=gnome-keyring.service gnome-session.service gnome-settings-daemon.service ibus.service
Requires=gnome-keyring.service gnome-session.service

[Service]
BusName=org.gnome.Shell
ExecStart=/usr/bin/gnome-shell
KillMode=process
Restart=always
RestartSec=1

[Install]
Alias=dbus-org.gnome.Shell.service
WantedBy=gnome.target
